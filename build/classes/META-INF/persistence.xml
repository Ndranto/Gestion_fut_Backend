<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Jax-rs_Gestion_Fut" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
		<class>model.Caracteristique</class>
		<class>model.CatalogueFut</class>
		<class>model.Categorie</class>
		<class>model.Client</class>
		<class>model.Inventaire</class>
		<class>model.Stockage</class>
		<class>model.Stocker</class>
		<class>model.StockerPK</class>
		<class>model.Transport</class>
		<class>model.UtiliserPK</class>
		<class>model.LigneInventaire</class>
		<class>model.Ville</class>
		<class>model.Utiliser</class>
		<class>model.Stockage_History</class>
		<class>model.Categoriser</class>
		<class>model.Bon</class>
		<class>model.Type</class>
		<class>model.Etat</class>
		<class>model.Sortie</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>
               <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
               <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/Inventory"/>
               <property name="javax.persistence.jdbc.user" value="postgres"/>
               <property name="javax.persistence.jdbc.password" value="root"/>
               <property name="eclipselink.ddl-generation.output-mode" value="database"/>
               <property name="eclipselink.logging.level" value="FINE"/>
		</properties>
	</persistence-unit>
</persistence>
